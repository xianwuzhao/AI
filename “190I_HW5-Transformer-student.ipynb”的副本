ag_news')\n","tokenized_datasets = dataset.map(tokenize_and_format, batched=True)\n","\n","# Format the dataset to output PyTorch tensors\n","tokenized_datasets.set_format(type='torch', columns=['input_ids', 'attention_mask', 'label'])\n","\n","# Create DataLoaders\n","train_loader = DataLoader(tokenized_datasets['train'], batch_size=32, shuffle=True)\n","eval_loader = DataLoader(tokenized_datasets['test'], batch_size=32)\n","\n","# Fine-tune BERT-mini on AGNews dataset\n","optimizer = AdamW(bertmini.parameters(), lr=5e-5)\n","criterion = nn.CrossEntropyLoss()\n","\n","device = torch.device('cuda' if torch.cuda.is_available() else 'cpu')\n","bertmini.to(device)\n","\n","# Training loop\n","for epoch in range(3):  # Number of epochs\n","    bertmini.train()\n","    for batch in train_loader:\n","        batch = {k: v.to(device) for k, v in batch.items()}\n","        outputs = bertmini(**batch)\n","        loss = outputs.loss\n","        loss.backward()\n","        optimizer.step()\n","        optimizer.zero_grad()\n","\n","# Save the model\n","torch.save(bertmini.state_dict(), best_bertmini_path)\n","\n","\n","# Load and pre\n","# only one line needed\n","\n","######################################\n","############ End of your code ########\n","#####################################"],"metadata":{"id":"MkGyjyevFglQ","colab":{"base_uri":"https://localhost:8080/","height":474,"referenced_widgets":["0e5d7384dd6b41dfb311be259ea41909","6ac4283c863d4a97b078e7ab7c6eb6cf","6f22a9fad4b7422d866473a3115a4fae","a904ae642ec84640b5a5b64656da1fe7","b8faf081dee840b482ed7bb7131e6079","b9fbee787a744ae48d19472523e753b6","6260093bd8ff4c209e7f42ccffe9eaf0","19c5bc48a56d404ca668c0077df10126","6030afbe31274666a965e63a9a490243","a6e1943595744c9ba3e2aa60afb1109b","8183b6d9ef7c4bb0a7e2f6668d26be14","9ad97f609e5f4d5fb1a166e3758858a9","6d0f1ae3bbbf411f8a5cfe282006ae96","105aa2d218b74aa3ba2a12253700a900","46044153e8d148c9895baac6a7026b70","dae98be9d05b4b419cabcc24b1729dc5","dfc6987d8a704ab68369ea