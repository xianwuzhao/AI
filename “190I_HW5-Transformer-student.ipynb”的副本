],\n","            'labels': label\n","        }"],"metadata":{"id":"h2lRcOjQxtSc"},"execution_count":null,"outputs":[]},{"cell_type":"code","source":["from transformers import BertTokenizer\n","\n","tokenizer = BertTokenizer.from_pretrained('bert-base-uncased')\n","print(\"Number of tokens:\", len(tokenizer))\n","print(\"Example tokenization: \")\n","rawstring = \"I love CS-190I machine learning class\"\n","print(\"Raw string: \", rawstring)\n","tokenized_sequence = tokenizer(rawstring, add_special_tokens=True, truncation=True, padding=\"max_length\", max_length=16, return_tensors='pt')['input_ids'][0]\n","print(\"Tokenized token id sequence: \\n\", tokenized_sequence)\n","print(\"Encode the token sequence back: \\n\", tokenizer.decode(tokenized_sequence))"],"metadata":{"colab":{"base_uri":"https://localhost:8080/"},"id":"qiserL1x3sVR","outputId":"f87e35c3-1280-4f8b-fab8-5c3bdc2c551e"},"execution_count":null,"outputs":[{"output_type":"stream","name":"stdout","text":["Number of tokens: 30522\n","Example tokenization: \n","Raw string:  I love CS-190I machine learning class\n","Tokenized token id sequence: \n"," tensor([  101,  1045,  2293, 20116,  1011, 11827,  2072,  3698,  4083,  2465,\n","          102,     0,     0,     0,     0,     0])\n","Encode the token sequence back: \n"," [CLS] i love cs - 190i machine learning class [SEP] [PAD] [PAD] [PAD] [PAD] [PAD]\n"]}]},{"cell_type":"code","source":["train_dataset = AGNewsDataset(agnews_train, tokenizer, 128)\n","print(\"Number of examples in train dataset: \", len(train_dataset))\n","print(\"An example item in train_dataset\")\n","print(train_dataset[42])"],"metadata":{"colab":{"base_uri":"https://localhost:8080/"},"id":"QSytUvcu4rnq","outputId":"c9e56e76-d5b2-4b33-f557-e2e8d3b62cbc"},"execution_count":null,"outputs":[{"output_type":"stream","name":"stdout","text":["Number of examples in train dataset:  20000\n","An example item in train_dataset\n","{'input_ids': tensor([  101,  4654,  1011,  2708,  1997, 15996,  36